<h1> <%= @city.name.capitalize %></h1>

<% if @city.photos.attached? %>
  <% @city.photos.each do |photo| %>
    <div class="py-2">
      <%= cl_image_tag photo.key, height: 300, width: 500, crop: :fill %>
    </div>
  <% end %>
<% end %>

